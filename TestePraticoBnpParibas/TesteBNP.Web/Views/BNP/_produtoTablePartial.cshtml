@model TesteBNP.Web.Models.ProdutoViewModel



@if (Model != null)
{
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Produtos
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>COD. PRODUTO</th>
                                <th>DES. PRODUTO</th>
                                <th>STA. STATUS</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.ProdutoLista)
                            {
                                <tr class="gradeA">
                                    <td>@item.CodProduto</td>
                                    <td>@item.DescricaoProduto</td>
                                    <td>@item.StaStatus</td>
                                </tr>
                            }
                        </tbody>
                    </table>

                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->


}
