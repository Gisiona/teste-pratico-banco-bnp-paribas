@model TesteBNP.Web.Models.MovimentoManualViewModel


@if (Model != null)
{
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Movimento Manual
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                            <thead>
                                <tr>
                                    <th>COD. MOVIMENTO</th>
                                    <th>MÊS</th>
                                    <th>ANO</th>
                                    <th>COD. PRODUTO</th>
                                    <th>DES. PRODUTO</th>
                                    <th>NUM. LANÇAMENTO</th>
                                    <th>DES. LANÇAMENTO</th>
                                    <th>VALOR</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.MovimentoManualLista)
                                {
                                <tr class="gradeA">
                                    <td>@item.CodigoMovimento</td>
                                    <td>@item.MesMovimento</td>
                                    <td>@item.AnoMovimento</td>
                                    <td>@item.Produtos.CodProduto</td>
                                    <td>@item.Produtos.DescricaoProduto</td>
                                    <td>@item.QtdeLancamento</td>
                                    <td>@item.DescricaoMovimento</td>
                                    <td>@item.ValorMovimento</td>
                                </tr>
                                }
                            </tbody>
                        </table>

                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->

    
}

